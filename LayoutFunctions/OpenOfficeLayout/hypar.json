{
  "$schema": "https://hypar.io/Schemas/Function.json",
  "id": "eeff6967-22a7-4490-b86c-5e842deaf219",
  "name": "Open Office Layout",
  "description": "Lay out desks with optional open collaboration areas distributed throughout.",
  "language": "C#",
  "model_dependencies": [
    {
      "autohide": false,
      "name": "Space Planning Zones",
      "optional": false
    },
    {
      "autohide": false,
      "name": "Circulation",
      "optional": true
    },
    {
      "autohide": false,
      "name": "Structure",
      "optional": true
    },
    {
      "autohide": false,
      "name": "Columns",
      "optional": true
    },
    {
      "autohide": false,
      "name": "Levels",
      "optional": true
    },
    {
      "name": "Floors",
      "optional": true
    },
    {
      "autohide": false,
      "name": "Conceptual Mass",
      "optional": true
    }
  ],
  "model_output": "Open Office Layout",
  "input_schema": {
    "type": "object",
    "properties": {
      "Grid Rotation": {
        "maximum": 360,
        "default": 0,
        "type": "number",
        "$hyparUnitType": "plane_angle",
        "$hyparOrder": 2,
        "minimum": 0
      },
      "Integrated Collaboration Space Density": {
        "description": "Increase this number to add more distributed collaboration spaces throughout open office areas.",
        "maximum": 1,
        "default": 0.2,
        "type": "number",
        "$hyparOrder": 1,
        "minimum": 0
      },
      "Aisle Width": {
        "$hyparStyle": "number",
        "description": "The width of the aisle between desks, measured from side to side.",
        "maximum": 2,
        "default": 1.0,
        "type": "number",
        "minimum": 0.5,
        "$hyparUnitType": "length",
        "$hyparPreferredUnits": {
          "metric": "mm",
          "imperial": "in"
        },
        "multipleOf": 0.05
      },
      "Back-to-Back Width": {
        "$hyparStyle": "number",
        "description": "The width of the space between rows of desks, measured from back of seat to back of seat.",
        "maximum": 2.0,
        "default": 1.0,
        "type": "number",
        "minimum": 0.5,
        "$hyparUnitType": "length",
        "$hyparPreferredUnits": {
          "metric": "mm",
          "imperial": "in"
        },
        "multipleOf": 0.05
      },
      "Desk Type": {
        "default": "Simple Desk - 29x70",
        "type": "string",
        "$hyparOrder": 0,
        "enum": [
          "Simple Desk - 24x48",
          "Simple Desk - 30x60",
          "Simple Desk - 29x70",
          "Simple Desk - 30x72",
          "L-Shaped",
          "Double Desk",
          "Enclosed Pair",
          "120째 Workstations - Pairs",
          "120째 Workstations - Continuous",
          "Custom"
        ]
      },
      "Column Avoidance Strategy": {
        "default": "Adaptive Grid",
        "type": "string",
        "enum": [
          "Adaptive Grid",
          "Cull",
          "None"
        ]
      },
      "Custom Workstation Properties": {
        "type": "object",
        "$hyparOrder": 0.5,
        "$hyparShowIf": {
          "conditions": [
            {
              "property": "Desk Type",
              "value": "Custom"
            }
          ]
        },
        "properties": {
          "Width": {
            "type": "number",
            "$hyparStyle": "number",
            "$hyparUnitType": "length",
            "minimum": 0.1,
            "default": 2.0
          },
          "Length": {
            "type": "number",
            "$hyparStyle": "number",
            "$hyparUnitType": "length",
            "minimum": 0.1,
            "default": 2.0
          }
        }
      }
    }
  },
  "overrides": {
    "Space Settings": {
      "dependency": "Space Planning Zones",
      "context": "[*discriminator=Elements.SpaceBoundary & Hypar Space Type = Open Office | Name = Open Office]",
      "identity": {
        "ParentCentroid": {
          "$ref": "https://schemas.hypar.io/Vector3.json"
        }
      },
      "paradigm": "edit",
      "schema": {
        "Grid Rotation": {
          "maximum": 360,
          "default": 0,
          "type": "number",
          "$hyparUnitType": "plane_angle",
          "minimum": 0
        },
        "Integrated Collaboration Space Density": {
          "description": "Increase this number to add more distributed collaboration spaces throughout open office areas.",
          "maximum": 1,
          "default": 0.2,
          "type": "number",
          "minimum": 0
        },
        "Aisle Width": {
          "$hyparStyle": "number",
          "description": "The width of the aisle between desks, measured from side to side.",
          "maximum": 2,
          "default": 1.0,
          "type": "number",
          "minimum": 0.5,
          "$hyparUnitType": "length",
          "$hyparPreferredUnits": {
            "metric": "mm",
            "imperial": "in"
          },
          "multipleOf": 0.05
        },
        "Back-to-Back Width": {
          "$hyparStyle": "number",
          "description": "The width of the space between rows of desks, measured from back of seat to back of seat.",
          "maximum": 2.0,
          "default": 1.0,
          "type": "number",
          "minimum": 0.5,
          "$hyparUnitType": "length",
          "$hyparPreferredUnits": {
            "metric": "mm",
            "imperial": "in"
          },
          "multipleOf": 0.05
        },
        "Desk Type": {
          "$hyparOrder": 0,
          "type": "string",
          "enum": [
            "Simple Desk - 24x48",
            "Simple Desk - 30x60",
            "Simple Desk - 29x70",
            "Simple Desk - 30x72",
            "L-Shaped",
            "Double Desk",
            "Enclosed Pair",
            "120째 Workstations - Pairs",
            "120째 Workstations - Continuous",
            "Custom"
          ],
          "default": "Simple Desk - 29x70"
        },
        "Custom Workstation Properties": {
          "$hyparOrder": 0.5,
          "type": "object",
          "$hyparShowIf": {
            "conditions": [
              {
                "property": "Desk Type",
                "value": "Custom"
              }
            ]
          },
          "properties": {
            "Width": {
              "$hyparStyle": "number",
              "type": "number",
              "$hyparUnitType": "length",
              "minimum": 0.1,
              "default": 2.0
            },
            "Length": {
              "$hyparStyle": "number",
              "type": "number",
              "$hyparUnitType": "length",
              "minimum": 0.1,
              "default": 2.0
            }
          }
        }
      },
      "verbs": {
        "edit": null,
        "revert": null
      },
      "default": {}
    },
    "Furniture Locations": {
      "perParent": "Level",
      "context": "[*discriminator=Elements.ElementInstance]",
      "identity": {
        "OriginalLocation": {
          "$ref": "https://schemas.hypar.io/Vector3.json"
        },
        "gltfLocation": {
          "type": "string"
        }
      },
      "paradigm": "edit",
      "schema": {
        "Transform": {
          "$ref": "https://schemas.hypar.io/Transform.json"
        }
      },
      "verbs": {
        "edit": null,
        "revert": null
      }
    }
  },
  "outputs": [
    {
      "unit_type": "none",
      "name": "Total Desk Count",
      "description": "The number of desks.",
      "type": "number"
    }
  ],
  "element_types": [
    "https://schemas.hypar.io/SpaceBoundary.json",
    "https://schemas.hypar.io/LevelElements.json",
    "https://schemas.hypar.io/CirculationSegment.json",
    "https://schemas.hypar.io/LevelVolume.json",
    "https://schemas.hypar.io/LabelConfiguration.json",
    "https://schemas.hypar.io/ProgramRequirement.json",
    "https://schemas.hypar.io/CollaborationSpaceBoundary.json"
  ],
  "repository_url": "https://github.com/hypar-io/function",
  "default_camera": {
    "named_position": "top",
    "projection": "orthographic"
  }
}